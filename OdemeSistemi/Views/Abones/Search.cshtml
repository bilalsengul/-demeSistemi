@model OdemeSistemi.Models.Abone
@{
    ViewBag.Title = "tcSearch";
}

<h2>tcSearch</h2>


<form action="@Url.Action("Search","Abones")" method="post">
    <input type="text" name="tc" placeholder="Tc Numarasını Giriniz." />
    <input type="text" name="vergi" placeholder="Vergi Numarasını Giriniz." />
    <input type="submit" value="Ara" />
</form>

@ViewBag.eror

@if (Model != null)
{
    <h>@Model.KullaniciAdi</h>
}
else if(ViewBag.error!=null) {
    <h1>@ViewBag.error</h1>
}
